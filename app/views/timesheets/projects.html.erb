<h1>Timesheets</h1>
 
<table class="table table-striped">
  <tr>
    <th>Document</th>
    <th>Status:</th>
  </tr>
  <% @timesheet.each do |t| %>
    <tr>
      <td><%= t.avatar %></td>
      <td>
      	<% if t.completed %>
      	Approved
      	<%else%>
      	Pending
      	<% end %>
      </td>
    </tr>
  <% end %>
</table>